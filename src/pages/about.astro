---
import BaseLayout from '../layouts/BaseLayout.astro';
import { pages } from '../data/pages';

const { meta, breadcrumb, scripts } = pages.about;
---

<BaseLayout {...meta} breadcrumb={breadcrumb} scripts={scripts}>
<section
        class="about3 section bg-color-body section-padding-top-bottom"
        data-bg-src="/assets/images/about/about-bg-3.png"
      >
        <div class="container position-relative">
          <div class="row g-4 gx-4 gx-xl-0">
            <div
              class="col-md-6 fade-anim"
              data-direction="left"
              data-delay="1.5"
            >
              <div class="about3__thumb">
                <div class="thumb-1">
                  <img
                    src="/img/illustrations/600x730_cpb-02.webp"
                    alt="about-thumb"
                   loading="lazy" decoding="async" width="600" height="730" />
                </div>
                <div class="thumb-2">
                  <img
                    src="/img/illustrations/600x730_cpb-03.webp"
                    alt="about-thumb"
                   loading="lazy" decoding="async" width="600" height="730" />
                </div>
              </div>
            </div>
            <div
              class="col-md-6 fade-anim"
              data-direction="right"
              data-delay="1.5"
            >
              <div class="about3__content section-header">
                <span class="section-sub-title text-uppercase">about me</span>
                <h2 class="section-title text-uppercase">
                  Jordan Lee engineering highlights
                </h2>

                <div class="about3__main-content">
                  <p>
                    I'm a product-focused full-stack developer who turns complex
                    requirements into reliable, user-centered product
                    experiences.
                  </p>
                  <div class="analysis">
                    <h6 class="title">Analysis</h6>
                    <ul>
                      <li>
                        <i class="fa-solid fa-badge-check"></i>visionary
                        product developer
                      </li>
                      <li>
                        <i class="fa-solid fa-badge-check"></i>web and mobile
                        apps
                      </li>
                      <li>
                        <i class="fa-solid fa-badge-check"></i>scalable
                        architecture
                      </li>
                    </ul>
                  </div>
                  <a href="/about" class="btn-primary row-margin-top">
                    <div class="btn-wrapper v2">
                      Let's talk
                      <span>
                        <i class="fa-sharp fa-solid fa-arrow-up-right"></i>
                        <i class="fa-sharp fa-solid fa-arrow-up-right"></i>
                        <i class="fa-sharp fa-solid fa-arrow-up-right"></i>
                      </span>
                    </div>
                  </a>
                  <div class="shape">
                    <img
                      src="/assets/images/about/about2-shap.png"
                      alt="shape"
                     loading="lazy" decoding="async"  width="132" height="114" />
                  </div>
                </div>
              </div>
            </div>
          </div>

          <div class="row g-4 row-margin-top">
            <div class="col-sm-6 col-lg-3">
              <div class="statistic-box2">
                <h2 class="counter-item">
                  <span class="odometer" data-odometer-final="12">0</span>
                  <em class="d-inline-block fst-normal">+</em>
                </h2>
                <span class="info">year of experience</span>
              </div>
            </div>
            <div class="col-sm-6 col-lg-3">
              <div class="statistic-box2">
                <h2 class="counter-item">
                  <span class="odometer" data-odometer-final="3">0</span>
                  <em class="d-inline-block fst-normal">k+</em>
                </h2>
                <span class="info">Happy Clients</span>
              </div>
            </div>
            <div class="col-sm-6 col-lg-3">
              <div class="statistic-box2">
                <h2 class="counter-item">
                  <span class="odometer" data-odometer-final="450">0</span>
                  <em class="d-inline-block fst-normal">+</em>
                </h2>
                <span class="info">project completed</span>
              </div>
            </div>
            <div class="col-sm-6 col-lg-3">
              <div class="statistic-box2">
                <h2 class="counter-item">
                  <span class="odometer" data-odometer-final="95 ">0</span>
                  <em class="d-inline-block fst-normal">+</em>
                </h2>
                <span class="info">Clients Satisfaction</span>
              </div>
            </div>
          </div>
        </div>
      </section>



      <section
