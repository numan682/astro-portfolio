                  <div class="skill-header">
                    <div class="skill-icon">
                      <img
                        src="/assets/images/skill/skill-icon-1-1.png"
                        alt="skill-icon"
                       loading="lazy" decoding="async"  width="41" height="40" />
                    </div>
                    <h4 class="skill-name">wordpress</h4>
                  </div>
                  <div class="skill">
                    <span class="title">Skill</span>
                    <div class="skill-bar">
                      <div class="skill-progress">
                        <span class="skill-count" data-skill-count="95"></span>
